{
  "compilerOptions": {
    "target": "ES5",
    "outDir": "./dist",
    "module": "ESNext",
    "moduleResolution": "Node",
    "sourceMap": true,
    "inlineSources": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  }
}
